{
  "name": "backend",
  "version": "1.0.1",
  "description": "FarmHub - Quản lý vật tư nông nghiệp",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"scripts/**/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/src/main",
    "runclean": "npm run format && npm run lint",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config test/jest.config.ts",
    "test:watch": "jest --config test/jest.config.ts --watch",
    "test:cov": "jest --config test/jest.config.ts --coverage",
    "test:unit": "jest --config test/config/jest-unit.config.ts",
    "test:integration": "jest --config test/config/jest-integration.config.ts",
    "test:e2e": "jest --config test/config/jest-e2e.config.ts",
    "test:e2e:auth": "jest test/e2e/auth.e2e-spec.ts --config test/config/jest-e2e.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config test/jest.config.ts",
    "test:security": "jest --config test/config/jest-unit.config.ts --testPathPattern=test/unit/(auth|guards)/.*\\.spec\\.ts$",
    "test:coverage": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:coverage2": "jest --config test/jest.config.ts --coverage --coverageReporters=html",
    "create-test-user": "ts-node -r tsconfig-paths/register scripts/create-test-user.ts",
    "check-users": "ts-node -r tsconfig-paths/register scripts/check-users.ts",
    "verify-password": "ts-node -r tsconfig-paths/register scripts/verify-password.ts",
    "create-user-example": "ts-node -r tsconfig-paths/register scripts/create-user-example.ts",
    "decode-jwt": "ts-node -r tsconfig-paths/register scripts/decode-jwt.ts",
    "check-sync": "ts-node -r tsconfig-paths/register scripts/sync-checker.ts",
    "monitor-db": "ts-node -r tsconfig-paths/register scripts/monitor-db-connection.ts",
    "test-singleton": "ts-node -r tsconfig-paths/register scripts/test-singleton.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cache-manager": "^7.0.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "glob": "^11.0.3",
    "ioredis": "^5.6.1",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^4.7.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.12.11",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  }
}
